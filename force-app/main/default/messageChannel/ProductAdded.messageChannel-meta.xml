<?xml version="1.0" encoding="UTF-8"?>
<LightningMessageChannel xmlns="http://soap.sforce.com/2006/04/metadata">
    <description>This event is fired when a product is added.</description>
    <isExposed>false</isExposed>
    <lightningMessageFields>
        <description>Product Item Added to Cart</description>
        <fieldName>productItem</fieldName>
    </lightningMessageFields>
    <lightningMessageFields>
        <description>Quantity Added to Cart</description>
        <fieldName>quantity</fieldName>
    </lightningMessageFields>
    <masterLabel>ProductAdded</masterLabel>
</LightningMessageChannel>
